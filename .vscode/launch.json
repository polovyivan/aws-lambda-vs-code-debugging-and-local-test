{
  "configurations": [
    {
      "type": "aws-sam",
      "request": "direct-invoke",
      "name": "aws-lambda-vs-code-debugging-and-local-test:testFunction",
      "invokeTarget": {
        "target": "template",
        "templatePath": "${workspaceFolder}/template.yaml",
        "logicalId": "testFunction"
      },
      "sam": {
        "buildArguments": ["--build-dir", "${workspaceFolder}/.sam-build/"],
        "localArguments": [
          "--template",
          "${workspaceFolder}/.sam-build/template.yaml"
        ]
      },
      "lambda": {
        "payload": {
          "json": {
            "Records": [
              {
                "messageId": "1",
                "receiptHandle": "MessageReceiptHandle",
                "body": "Hello from SQS1!",
                "attributes": {
                  "ApproximateReceiveCount": "1",
                  "SentTimestamp": "1523232000000",
                  "SenderId": "123456789012",
                  "ApproximateFirstReceiveTimestamp": "1523232000001"
                },
                "messageAttributes": {},
                "md5OfBody": "7b270e59b47ff90a553787216d55d91d",
                "eventSource": "aws:sqs",
                "eventSourceARN": "arn:{partition}:sqs:{region}:123456789012:MyQueue",
                "awsRegion": "{region}"
              }
            ]
          }
        },
        "environmentVariables": {
          "BUCKET_ENDPOINT": "http://192.168.122.1:4566",
          "BUCKET_NAME": "tutorial-bucket",
          "REGION": "us-east-1"
        }
      }
    }
  ]
}
